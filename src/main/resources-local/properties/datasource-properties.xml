<properties>
    <entry key="jdbc.driverClassName">oracle.jdbc.driver.OracleDriver</entry>
    <entry key="jdbc.initialSize">1</entry>
    <entry key="jdbc.defaultautocommit">false</entry>
    <entry key="jdbc.maxActive">2</entry>
    <entry key="jdbc.maxIdle">2</entry>
    <entry key="jdbc.maxWait">-1</entry>
    <entry key="jdbc.testOnBorrow">true</entry>
    <entry key="jdbc.testWhileIdle">true</entry>
    <entry key="jdbc.timeBetweenEvictionRunsMillis">30000</entry>
    <entry key="jdbc.validationQuery">SELECT 1 FROM DUAL</entry>
    <entry key="jdbc.poolPreparedStatements">true</entry>
</properties>
